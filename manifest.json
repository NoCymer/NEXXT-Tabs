{
    "manifest_version": 3,
    "version": "1.1",
    "name": "NEXXT TABS",
    "description": "NEXXT Tabs will please every anime fans. Replace your basic tab to a minimalist customizable anime themed tab.",
    "action": {
        "default_icon": "icon.png"
    },
    "offline_enabled": true,
    "background": {
      "service_worker": "js/serviceWorker.js"
    },
    "chrome_url_overrides": {
       "newtab": "index.html"
    },
    "permissions": [],
    "icons": {
      "128": "icon128.png"
    },
    "author": "NoCymer"

}